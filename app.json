{
  "expo": {
    "name": "audio-fairytales",
    "slug": "fairy-tales",
    "description": "This library of audio fairy tales",
    "version": "1.0.1",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios.olegp306.fairytales"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "android.olegp306.fairytales"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
